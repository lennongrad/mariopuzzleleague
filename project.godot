config_version=5

[application]

config/name="Mario Puzzle League"
config/description="The Puzzle League sensation has landed in Dinosaur Land. Help Mario compete against Bowser and other foes in a test of dexterity, with a demanding race of strategic moves and thoughtful analysis."
run/main_scene="res://scenes/Main.tscn"
config/features=PackedStringArray("4.2")
boot_splash/image="res://graphics/splash.png"
config/icon="res://graphics/characters/lip/trash.png"
config/windows_native_icon="res://graphics/windows_icon.ico"

[display]

window/size/viewport_width=256
window/size/viewport_height=224
window/size/window_width_override=512
window/size/window_height_override=448
window/stretch/mode="viewport"

[gdnative]

singletons=["res://git_api.gdnlib"]

